{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "id": "http://example.com/example.json",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "title": {
          "title": "Card Title",
          "type": "string",
          "default": "Choose type",
          "enum": ["Leadership MLA", "Leadership MP"]
        },
        "district": {
          "title": "District",
          "type": "string"
        },
        "details": {
          "id": "/properties/data/properties/questions",
          "title": "MLA Details",
          "items": {
            "id": "/properties/data/properties/questions/items",
            "title": "MLA",
            "separator": "#",
            "hideTitle": true,
            "properties": {
              "assembly": {
                "title": "Assembly",
                "id": "/properties/data/properties/questions/items/properties/question",
                "type": "string",
                "default": "None"
              },
              "name": {
                "type": "string",
                "title": "Name",
                "default": ""
              },
              "party": {
                "type": "string",
                "title": "Party",
                "default": "Party Name",
                "enum": ["BSP", "BJP"]
              },
              "assets": {
                "type": "string",
                "title": "Assets",
                "default": ""
              },
              "education": {
                "type": "string",
                "title": "Education",
                "default": ""
              }
            },
            "required": [
              "education",
              "party",
              "name",
              "assembly"
            ],
            "minItems": 1,
            "addButtonText": "Add another MLA",
            "type": "object"
          },
          "minItems": 1,
          "type": "array"
        }
      }
    }
  },
  "type": "object"
}
